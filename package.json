{
  "name": "fluencyjet-sentence-master",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.9.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.js",
    "build": "npm --prefix client install --omit=dev && npm --prefix client run build",
    "postinstall": "prisma generate --schema=server/prisma/schema.prisma",
    "deploy:migrate": "prisma migrate deploy --schema=server/prisma/schema.prisma",
    "start": "NODE_ENV=production node server/index.js",
    "check": "tsc",
    "seed": "node server/prisma/seed.js",
    "reset:schema": "node server/db/resetSchema.js",
    "serve:dist": "npx serve client/dist --listen 8080",
    "preview": "npm run build && npm run serve:dist",
    "patch:enum": "node server/scripts/patch-eventtype-enum.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/client": "^6.18.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.39.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "prisma": "^6.18.0",
    "serve": "^14.2.5",
    "tsx": "^4.20.5",
    "ws": "^8.18.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "20.16.11",
    "drizzle-kit": "^0.31.4",
    "typescript": "5.6.3"
  },
  "prisma": {
    "seed": "node server/prisma/seed.js"
  }
}
