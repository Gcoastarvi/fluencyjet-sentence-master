{
  "name": "fluencyjet-sentence-master",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.9.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.js",
    "prebuild": "rm -rf dist && mkdir -p dist",
    "build": "cd client && npm install --no-audit --no-fund && npx vite build && cd .. && npx esbuild server/index.js --platform=node --bundle --format=esm --outfile=dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/client": "^6.17.1",
    "body-parser": "^2.2.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.39.1",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "memorystore": "^1.6.7",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.2",
    "tsx": "^4.20.5",
    "esbuild": "^0.25.0"
  },
  "devDependencies": {
    "@types/node": "20.16.11",
    "drizzle-kit": "^0.31.4",
    "typescript": "5.6.3"
  }
}
